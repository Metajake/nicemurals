.justify-left
  justify-content:left

header
  .icon
    margin-right:1rem

#home-link
  position:relative
  left:-200px
  animation: arrive 2s forwards
  &.move
    animation: move 4s

@keyframes arrive
  0%
    left:-200px
  100%
    left:0px

@keyframes move
  0%
    left:0px
  100%
    left:1000px
